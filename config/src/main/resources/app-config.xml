<?xml version="1.0" encoding="UTF-8"?>
<application xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://appconfig.aura.nav.no"
             xsi:schemaLocation="http://appconfig.aura.nav.no http://maven.adeo.no/xsd/appconfig.xsd">

    <name>fastlegerest</name>

    <selftest>
        <path>fastlegerest/internal/selftest</path>
        <humanReadablePath>fastlegerest/internal/selftest</humanReadablePath>
    </selftest>

    <artifacts>
        <ear groupId="no.nav.syfo" artifactId="fastlegerest-ear"/>
    </artifacts>

    <resources>
        <webservice alias="ekstern:helse_fastlegeinformasjon" mapToProperty="fastlegeinformasjon.endpoint" />
        <webservice alias="virksomhet:Brukerprofil_v3" mapToProperty="brukerprofilv3.endpoint"/>
        <webservice alias="virksomhet:Diskresjonskode_v1" mapToProperty="diskresjonskode.endpoint"/>
        <webservice alias="virksomhet:EgenAnsatt_v1" mapToProperty="egenAnsatt.endpoint"/>

        <ldap alias="ldap" mapToProperty="ldap"/>

        <baseUrl alias="isso-host"/>
        <baseUrl alias="isso-jwks"/>
        <baseUrl alias="isso-issuer"/>
        <credential alias="isso-rp-user"/>
        <rest alias="veilarblogin.redirect-url" mapToProperty="oidc-redirect"/>
    </resources>

    <exposed-services>
        <exposedRest path="/fastlegerest/api/fastlege/v1" name="fastlegeapi" description="Fastlege FSS REST-API"/>
    </exposed-services>


    <security serviceUserResourceAlias="srvfastlegerest" />

    <loadBalancer isAlive="fastlegerest/internal/isAlive"/>
</application>